<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="WindAction.lua"/>
	<Frame name="WindActionRC" virtual="true" hidden="true">
		<Size>
			<AbsDimension x="36" y="36"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativeTo="$parent" relativePoint="CENTER"/>
		</Anchors>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="$parentCCount" font="Fonts\2002.TTF" outline="NORMAL" hidden="true"/>
			</Layer>
		</Layers>
	</Frame>
	<Frame name="WindAction" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>WindAction_OnLoad();</OnLoad>
		</Scripts>
	</Frame>
</Ui>