<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="WindActionPet.lua"/>
	<Frame name="WindActionPet" frameStrata="MEDIUM" toplevel="true" enableMouse="false" parent="UIParent" hidden="true">
		<Frames>
			<Frame name="WindActionPetBorder" inherits="WindBorder"/>
		</Frames>
		<Scripts>
			<OnLoad>WindActionPet_OnLoad(self);</OnLoad>
			<OnShow>WindActionPet_OnShow(self);</OnShow>
			<OnHide>WindActionPet_OnHide(self);</OnHide>
			<OnEvent>WindActionPet_OnEvent(self, event, ...);</OnEvent>
		</Scripts>
	</Frame>
</Ui>